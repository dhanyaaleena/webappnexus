
pipeline{
	agent any
	
	stages{
		stage ('Compile Stage') {
			steps {
				withMaven(maven : 'maven-3.6.3'){
					sh 'mvn clean compile'
				}
			}
		}
		stage ('Testing Stage') {
			steps {
				withMaven(maven : 'maven-3.6.3'){
					sh 'mvn test'
				}
			}
		}
		stage ('QA with SonarQube') {
			steps {
				withSonarQubeEnv('sonar-8'){
					sh 'mvn clean package sonar:sonar'
				}
			}
		}
		stage ('Upload War To nexus'){
			steps{
				sh 'mvn clean package'
			}
		}
	} 
}
		